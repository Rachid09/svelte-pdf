{
  "name": "svelte-pdf",
  "version": "1.0.13",
  "description": "svelte-pdf provides a component for rendering PDF documents using PDF.js",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-check --tsconfig ./jsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.7.1",
    "svelte2tsx": "^0.5.13",
    "typescript": "^4.7.4",
    "vite": "^3.0.0"
  },
  "dependencies": {
    "file-saver": "^2.0.5",
    "pdfjs-dist": "^2.15.349"
  },
  "exports": {
    ".": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vinodnimbalkar/svelte-pdf.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vinodnimbalkar/svelte-pdf/issues"
  },
  "homepage": "https://github.com/vinodnimbalkar/svelte-pdf#readme",
  "keywords": [
    "pdf",
    "svelte-pdf",
    "pdfjs",
    "pdf-print",
    "pdf-zoom",
    "pdf-rotation",
    "pdf-navigation"
  ]
}
